<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd">


    <dubbo:application name="dubbo-demo"/>
    <dubbo:protocol name="dubbo" port="20880"/>
    <dubbo:registry protocol="dubbo" address="zookeeper://localhost:2182" check="true"/>
    <!--<dubbo:reference interface="com.nee.demo.dubbo.api.DemoService"/>-->
    <dubbo:service interface="com.nee.demo.dubbo.api.DemoService" ref="demoService"/>
    <bean name="demoService" class="com.nee.demo.dubbo.provider.DemoServiceImpl"/>
</beans>